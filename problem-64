#include <stdio.h>

int main() {
    int n, i, key, low, high, mid;
    int a[100];

    scanf("%d", &n);

    for(i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }

    scanf("%d", &key);

    low = 0;
    high = n - 1;

    while(low <= high) {
        mid = (low + high) / 2;

        if(a[mid] == key) {
            printf("Found at index %d\n", mid);
            return 0;
        }
        else if(a[mid] > key) {
            high = mid - 1;
        }
        else {
            low = mid + 1;
        }
    }

    printf("Not Found\n");

    return 0;
}

/*
Input:
5
1 3 5 7 9
7

Output:
Found at index 3
*/
