#include <stdio.h>
#include <math.h>

int main() {
    int n, a[10][10];
    scanf("%d", &n);

    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            scanf("%d", &a[i][j]);

    int trace = 0;
    float sumSquares = 0;

    for (int i = 0; i < n; i++) {
        trace += a[i][i];
        for (int j = 0; j < n; j++) {
            sumSquares += a[i][j] * a[i][j];
        }
    }

    float normal = sqrt(sumSquares);

    printf("Trace = %d\n", trace);
    printf("Normal = %.2f\n", normal);

    return 0;
}

/*
Input:
3
1 2 3
4 5 6
7 8 9

Output:
Trace = 15
Normal = 16.88
*/
